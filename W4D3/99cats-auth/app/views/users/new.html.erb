<h1>New User</h1>

<form action="/users" method="post">
  <input
        type="hidden"
        name="authenticity_token"
        value="<%= form_authenticity_token %>">

  <label for="user_name">User Name:</label>
  <input
  type="text"
  name="user[user_name]"
  id ="user_name"
  >

  <label for="password">Password:</label>
  <input
  type="text"
  name="user[password]"
  id ="user_password"
  >

  <input type="submit" value='Create User!'>


</form>
